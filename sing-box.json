{
    "log": {
        "log": {
            "disabled": false,
            "level": "info",
            "output": "box.log",
            "timestamp": true
        }
    },
    "dns": {
        "servers": [
            {
                "tag": "dns_direct",
                "address": [
                    "223.5.5.5",
                    "119.29.29.29"
                ],
                "address_strategy": "prefer_ipv4 ",
                "strategy": "prefer_ipv4 ",
                "detour": "直连"
            },
            {
                "tag": "dns_proxy",
                "address": [
                    "https://dns.cloudflare.com/dns-query",
                    "https://dns.google/dns-query"
                ],
                "address_resolver": "dns_direct",
                "address_strategy": "prefer_ipv4 ",
                "strategy": "prefer_ipv4 ",
                "detour": "节点选择"
            },
            {
                "tag": "dns_block",
                "address": "rcode://name_error"
            }
        ],
        "rules": [
            {
                "outbound": "all",
                "server": "dns_direct"
            },
            {
                "clash_mode": "Direct",
                "server": "dns_direct",
                "disable_cache": true
            },
            {
                "clash_mode": "Global",
                "server": "dns_proxy",
                "disable_cache": true
            },
            {
                "rule_set": "geoip-cn",
                "outbound": "direct"
            },
            {
                "rule_set": "geosite-cn",
                "outbound": "direct"
            }
        ]
    },
    "inbounds": [
        {
            "type": "tun",
            "tag": "tun-in",
            "address": "172.18.0.1/30",
            "interface_name": "tun0",
            "mtu": 9000,
            "gso": true,
            "auto_route": true,
            "auto_redirect": true,
            "stack": "system"
        }
    ],
    "route": {
        "rules": [
            {
                "action": "sniff",
                "sniffer": [
                    "http",
                    "tls",
                    "quic",
                    "dns",
                    "bittorrent"
                ],
                "timeout": "500ms"
            },
            {
                "type": "logical",
                "mode": "or",
                "rules": [
                    {
                        "port": 53
                    },
                    {
                        "protocol": "dns"
                    }
                ],
                "action": "hijack-dns"
            },
            {
                "type": "inline",
                "protocol": [
                    "bittorrent",
                    "dht"
                ],
                "outbound": "direct"
            },
            {
                "ip_is_private": true,
                "action": "route",
                "outbound": "direct"
            },
            {
                "clash_mode": "Global",
                "action": "route",
                "outbound": "GLOBAL"
            },
            {
                "clash_mode": "Direct",
                "action": "route",
                "outbound": "direct"
            }
        ],
        "rule_set": [
            {
                "tag": "apple_ip",
                "type": "remote",
                "format": "binary",
                "url": "https://ghfast.top/https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/refs/heads/sing/geo-lite/geoip/apple.srs",
                "download_detour": "direct"
            }
        ]
    },
    "experimental": {
        "cache_file": {
            "enabled": true,
            "store_fakeip": true
        },
        "clash_api": {
            "external_controller": "0.0.0.0:9090",
            "external_ui": "ui",
            "external_ui_download_url": "",
            "external_ui_download_detour": ""
        }
    },
    "outbounds": [
        {
            "tag": "direct",
            "type": "direct"
        },
        {
            "tag": "all-auto",
            "type": "urltest",
            "outbounds": [],
            "url": "https://www.gstatic.com/generate_204",
            "interval": "5m",
            "tolerance": 50
        },
        {
            "tag": "proxy",
            "type": "selector",
            "outbounds": [
                "hk-auto",
                "tw-auto",
                "jp-auto",
                "sg-auto",
                "us-auto",
                "all-auto",
                "direct"
            ],
            "default": "all-auto"
        },
        {
            "tag": "GLOBAL",
            "type": "selector",
            "outbounds": [
                "direct",
                "proxy",
                "hk-auto",
                "tw-auto",
                "jp-auto",
                "sg-auto",
                "us-auto",
                "all-auto"
            ],
            "default": "direct"
        },
        {
            "tag": "hk-auto",
            "type": "urltest",
            "outbounds": [],
            "url": "https://www.gstatic.com/generate_204",
            "interval": "5m",
            "tolerance": 50
        },
        {
            "tag": "tw-auto",
            "type": "urltest",
            "outbounds": [],
            "url": "https://www.gstatic.com/generate_204",
            "interval": "5m",
            "tolerance": 50
        },
        {
            "tag": "jp-auto",
            "type": "urltest",
            "outbounds": [],
            "url": "https://www.gstatic.com/generate_204",
            "interval": "5m",
            "tolerance": 50
        },
        {
            "tag": "sg-auto",
            "type": "urltest",
            "outbounds": [],
            "url": "https://www.gstatic.com/generate_204",
            "interval": "5m",
            "tolerance": 50
        },
        {
            "tag": "us-auto",
            "type": "urltest",
